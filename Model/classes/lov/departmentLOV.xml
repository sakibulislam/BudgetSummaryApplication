<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="departmentLOV"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="lov.departmentLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="userID"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
    <TransientExpression><![CDATA[adf.context.sessionScope.userId]]></TransientExpression>
  </Variable>
  <SQLQuery>
    <![CDATA[select distinct c.COST_CENTER,c.COST_CENTER_NAME from MJ_BUDGET_COST_CENTER c
WHERE 1=1
AND COST_CENTER in ( select cost_center from MJ_PO_BUDGET_USER_DETAILS where user_id = :userID  )
ORDER BY COST_CENTER]]>
  </SQLQuery>
  <ViewAttribute
    Name="CostCenter"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="COST_CENTER"
    Expression="COST_CENTER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="5"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="20"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="CostCenterName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="COST_CENTER_NAME"
    Expression="COST_CENTER_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="20"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewCriteria
    Name="departmentLOVCriteria"
    ViewObjectName="lov.departmentLOV"
    Conjunction="AND">
    <Properties>
      <CustomProperties>
        <Property
          Name="displayOperators"
          Value="InAdvancedMode"/>
        <Property
          Name="autoExecute"
          Value="true"/>
        <Property
          Name="showInList"
          Value="true"/>
        <Property
          Name="allowConjunctionOverride"
          Value="true"/>
        <Property
          Name="mode"
          Value="Basic"/>
      </CustomProperties>
    </Properties>
    <ViewCriteriaRow
      Name="vcrow2"
      UpperColumns="1">
      <ViewCriteriaItem
        Name="departmentLOVCriteria_vcrow2_CostCenter"
        ViewAttribute="CostCenter"
        Operator="STARTSWITH"
        Conjunction="AND"
        Required="Optional"/>
      <ViewCriteriaItem
        Name="departmentLOVCriteria_vcrow2_CostCenterName"
        ViewAttribute="CostCenterName"
        Operator="CONTAINS"
        Conjunction="OR"
        Required="Optional"/>
    </ViewCriteriaRow>
  </ViewCriteria>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
